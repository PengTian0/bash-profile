{
  "http.proxy": "http://3.20.128.6:88",
  "http.proxyStrictSSL": false,
  "editor.fontSize": 10,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "solid",
  "editor.minimap.enabled": false,
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.vscode": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.pyc": true,
    "**/tmp": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/bower_components/**": true,
    "**/dist/**": true
  },
  "window.menuBarVisibility": "hidden",
  "window.openFilesInNewWindow": "default",
  "update.channel": "none",
  "vim.leader": ";",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors": {
    "normal": "#0A68CC",
    "insert": "#5f0000",
    "visual": "#5f00af",
    "visualline": "#005f87",
    "visualblock": "#86592d",
    "replace": "#000000"
  },
  "python.formatting.provider": "yapf",
  "python.linting.pylintEnabled": false,
  "java.home": "/Library/Java/JavaVirtualMachines/jdk-9.0.1.jdk/Contents/Home",
  "java.errors.incompleteClasspath.severity": "ignore",
  "workbench.iconTheme": "vscode-icons",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f87",
    "statusBar.noFolderBackground": "#005f87",
    "statusBar.debuggingBackground": "#005f87"
  },
  "vim.disableAnnoyingNeovimMessage": true,
  "extensions.ignoreRecommendations": true,
  "python.venvPath": "~/.envs",
  "workbench.colorTheme": "Visual Studio Light"
}